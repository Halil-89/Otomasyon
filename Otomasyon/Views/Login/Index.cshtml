	<div class="pop-up">
		<div id="small-dialog2" class="mfp-hide book-form">

			@*@await Component.InvokeAsync("CariKayit")*@

			@await Html.PartialAsync("_PartialCariKayitView")

		</div>
	</div>
	<!-- //register form popup-->
	<!-- js -->
	<script type="text/javascript" src="~/weblogin/js/jquery-2.1.4.min.js"></script>

    
 
    
	<!-- //js -->
	<!--popup-js-->
	<script src="~/weblogin/js/jquery.magnific-popup.js" type="text/javascript"></script>

	<script>
		var jQueryCalis = $.noConflict(true);
		(function ($) {
			$('.popup-with-zoom-anim').magnificPopup({
				type: 'inline',
				fixedContentPos: false,
				fixedBgPos: true,
				overflowY: 'auto',
				closeBtnInside: true,
				preloader: false,
				midClick: true,
				removalDelay: 300,
				mainClass: 'my-mfp-zoom-in'
			});
		})(jQueryCalis);
	</script>


