@model Admin

<div class="login-form login-form-left">
	<div class="agile-row">
		<h3>Personel Girişi</h3>

		<div class="clear"></div>
		<div class="login-agileits-top">
			@using (Html.BeginForm("PartialPersonelGirisView", "Login", FormMethod.Post))
			 {
				<input type="text" class="name" asp-for="KullaniciAd" Placeholder="Kullanıcı Adı" required="" />
				<input type="password" class="password" asp-for="Sifre" Placeholder="Şifre" required="" />
				<input type="submit" value="Giriş Yap">

			}
				
		</div>
	</div>
</div>