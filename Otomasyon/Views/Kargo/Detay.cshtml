@model IEnumerable<KargoTakip>

@{
    ViewData["Title"] = "Detay";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<!DOCTYPE html>
<html lang="en">
<head>
    <title></title>
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
</head>
<table class="table table-bordered" style="margin-top:20px">
    <thead>
        <tr>
          
            <td>Acıklama</td>
            <td>Tarih</td>
            



        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
              
                <td>@item.Aciklama</td>
                <td>@item.TarihZaman</td>
               

              @*  <td>
                    <a class="btn btn-success" asp-controller="Cari" asp-action="Güncelle" asp-route-id=@item.CariId>Güncelle</a>
                </td>*@
                
            </tr>



        }

    </tbody>



