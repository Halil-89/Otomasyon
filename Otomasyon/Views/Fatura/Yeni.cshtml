@model FaturaKalem
@{
    ViewData["Title"] = "Yeni";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<!DOCTYPE html>
<html lang="tr">
<head>
    <title>Fatura Ekleme Sayfası</title>
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
</head>
<h2>Fatura Kalem Ekleme Sayfası</h2>
<br />
<body>

    <form class="form-control" method="post">
      

        <label asp-for="Aciklama">Acıklama</label>
        <input type="text" asp-for="Aciklama" class="form-control" />
        <span asp-validation-for="@Model.Aciklama" class="text-danger"></span>

        <label asp-for="Miktar">Miktar</label>
        <input type="text" asp-for="Miktar" class="form-control" />
        <span asp-validation-for="@Model.Miktar" class="text-danger"></span>

        <label asp-for="BirimFiyat">Fiyat</label>
        <input type="text" asp-for="BirimFiyat" class="form-control" />
        <span asp-validation-for="@Model.BirimFiyat" class="text-danger"></span>

        <label asp-for="Tutar">Tutar</label>
        <input type="text" asp-for="Tutar" class="form-control" />
        <span asp-validation-for="@Model.Tutar" class="text-danger"></span>

        <label asp-for="FaturaId">Id</label>
        <input type="text" asp-for="FaturaId" class="form-control" />
        <span asp-validation-for="@Model.FaturaId" class="text-danger"></span>

      
        <br />
        <button class="btn btn-success">Kaydet</button>

    </form>
</body>
</html>



